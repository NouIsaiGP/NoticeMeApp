<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.NoticeMe" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#232e3a</item>
        <item name="colorPrimaryVariant">#123352</item>
        <item name="colorOnPrimary">#0f4b64</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>
        <!-- Customize your theme here. -->
    </style>



    <style name="SplashTheme" parent="Theme.NoticeMe">
        <item name="android:windowBackground">@drawable/splashscreen</item>
    </style>

    <style name="gradiat" parent="Theme.NoticeMe">
        <item name="android:windowBackground">@drawable/gradient</item>
    </style>
</resources>